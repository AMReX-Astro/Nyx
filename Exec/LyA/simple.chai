var procmap = ProcMap()

if(procmap.group() == "world")
{
    var nyx      = load("./Nyx3d.llvm.PROF.MPI.ex inputs", procmap)
    var analysis = load("../Henson/sample-analysis/sample-analysis3d.llvm.PROF.MPI.ex", procmap)

    puts("Nyx loaded\n")

    while(nyx.proceed())
    {
        puts("Time step\n")
        analysis.proceed()
    }
}
